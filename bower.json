{
  "name": "bluemix-components",
  "version": "4.3.1-alpha",
  "description": "Bluemix Components is a collection of commonly used, reusable UI elements for developers to use in their prototypes and production work for Bluemix -- the scope of this project may expand to be used more widely throughout IBM.",
  "main": "_styles.scss",
  "dependencies": {
    "bluemix-icons": "^0.2.2",
    "IBM-Design-Colors": "ibm-colors#~1.5.1"
  },
  "keywords": [
    "bluemix",
    "component",
    "components"
  ],
  "license": "MIT",
  "ignore": [
    ".git",
    "dev.{scss, css}",
    "dev-colors.{scss, css}",
    "dev-typography.{scss, css}",
    "bower_components",
    "node_modules",
    "examples",
    ".babelrc",
    ".bowerrc",
    ".editorconfig",
    ".eslintignore",
    ".eslintrc",
    ".sass-lint.yml",
    "CORE-TEAM.md",
    "gulpfile.js",
    "server.js",
    "views"
  ]
}
