{
  "name": "@console/bluemix-components",
  "version": "6.12.0",
  "description": "Bluemix Components is a component library for Bluemix",
  "jsnext:main": "consumables/js/es2015/index",
  "main": "consumables/js/umd/lib/index",
  "maintainers": [
    {
      "name": "Brian Han",
      "email": "bthan@us.ibm.com"
    }
  ],
  "scripts": {
    "commitmsg": "validate-commit-msg",
    "prebuild": "gulp clean",
    "build": "gulp build",
    "serve": "nodemon -e dust,js server.js",
    "predev": "gulp build:dev",
    "dev": "gulp serve & npm run serve",
    "lint": "gulp lint",
    "test": "gulp test",
    "start": "node server.js",
    "test:unit": "gulp test:unit",
    "test:a11y": "gulp test:a11y"
  },
  "engines": {
    "node": "~4.0.0",
    "npm": "~3.0.0"
  },
  "dependencies": {
    "@console/bluemix-icons": "3.5.1",
    "adaro": "1.0.4",
    "bluebird": "~3.1.1",
    "eslint": "^3.0.0",
    "express": "4.13.4",
    "globby": "4.0.0",
    "lodash.debounce": "^4.0.6"
  },
  "devDependencies": {
    "babel-core": "^6.22.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "~6.2.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.0.0",
    "babel-runtime": "^6.22.0",
    "browser-sync": "~2.10.0",
    "chai": "~3.4.1",
    "core-js": "^2.4.0",
    "custom-event": "^1.0.0",
    "cz-conventional-changelog": "^1.2.0",
    "del": "~2.0.2",
    "es6-weak-map": "^2.0.1",
    "eslint": "^3.0.0",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "~3.9.0",
    "gulp-autoprefixer": "~3.0.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.0",
    "gulp-jsdoc3": "^0.2.1",
    "gulp-rename": "~1.2.2",
    "gulp-sass": "2.3.1",
    "gulp-sass-lint": "~1.1.1",
    "gulp-sourcemaps": "~1.6.0",
    "gulp-util": "~3.0.7",
    "html-loader": "^0.4.4",
    "husky": "^0.12.0",
    "karma": "^1.0.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "~1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.1.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "^2.0.0",
    "lolex": "1.4.0",
    "merge-stream": "^1.0.0",
    "minimist": "~1.2.0",
    "mocha": "~2.4.4",
    "nodemon": "1.9.1",
    "phantomjs-prebuilt": "^2.1.3",
    "scss-to-json": "1.1.0",
    "sinon": "1.17.3",
    "sinon-chai": "2.8.0",
    "svgxuse": "1.1.22",
    "validate-commit-msg": "^2.8.2",
    "webpack": "^2.0.0"
  },
  "repository": {
    "type": "git"
  },
  "files": [
    "src/**/*"
  ],
  "keywords": [
    "bluemix",
    "component",
    "components",
    "css",
    "html",
    "ibm",
    "javascript",
    "js",
    "library",
    "pattern",
    "patterns",
    "sass",
    "scss",
    "style guide"
  ],
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "Subject does not match subject pattern!"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
